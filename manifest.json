{
    "name": "My PWA",
    "short_name": "PWA",
    "start_url": ".",
    "display": "standalone",
    "background_color": "#ffffff",
    "description": "A simple PWA example",
    "icons": [
        {
            "src": "icon/lowres.webp",
            "sizes": "48x48",
            "type": "image/webp"
        },
        {
            "src": "icon/lowres.png",
            "sizes": "48x48",
            "type": "image/png"
        },
        {
            "src": "icon/hd_hi.ico",
            "sizes": "72x72 96x96 128x128 256x256"
        },
        {
            "src": "icon/hd_hi.svg",
            "sizes": "72x72"
        }
    ],
    "serviceworker": {
        "src": "service-worker.js",
        "scope": "/",
        "use_cache": true,
        "cache": [
            "/node_modules/*"
        ]
    }
}