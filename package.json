{
  "name": "gener",
  "version": "1.0.0",
  "description": "gener",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-builder --dir",
    "make": "electron-builder"
  },
  "author": "panda443212",
  "license": "ISC",
  "build": {
    
    "appId": "com.example.gener",
    "productName": "Gener",
    "icon": "app-icon/app-icon",
    "compression": "store",
    "files": [
      "**/*",
      "!**/7z.exe"
    ],
    "mac": {
      "target": "dmg",
      "icon": "app-icon/app-icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "app-icon/app-icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "app-icon/app-256.png"
    },
    "deb": {
      "icon": "app-icon/app-256.png"
    },
    "rpm": {
      "icon": "app-icon/app-256.png"
    },
    "asar": true
  },
  "devDependencies": {
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1"
  }
}
